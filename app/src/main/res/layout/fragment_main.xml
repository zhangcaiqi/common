<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:ignore="MissingDefaultResource">
    <Button
        android:id="@+id/send_msg_btn"
        android:text="发送"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"/>
    <Button
        android:id="@+id/jump_to_page"
        android:text="分页测试"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/send_msg_btn"/>
    <Button
        android:id="@+id/jump_to_complex"
        android:text="复合列表"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/jump_to_page"/>
    <Button
        android:id="@+id/jump_to_image"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="跳转到图片框架"
        app:layout_constraintTop_toBottomOf="@id/jump_to_complex"/>
    <Button
        android:id="@+id/jump_to_indicator"
        android:text="跳转到指示器"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/jump_to_image"/>
    <Button
        android:id="@+id/jump_to_tab"
        android:text="跳转到TabActivity"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/jump_to_indicator"/>
    <Button
        android:text="跳转到Option"
        android:id="@+id/jump_to_option"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/jump_to_tab"
        />
    <Button
        android:id="@+id/jump_to_page_food"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/jump_to_option"
        />
</androidx.constraintlayout.widget.ConstraintLayout>